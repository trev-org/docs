{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Windsurf Docs",
  "colors": {
    "primary": "#E997E6",
    "light": "#07C983",
    "dark": "#011C42"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Windsurf",
        "groups": [
          {
            "group": "Intro to Concepts",
            "pages": ["introduction", "quickstart", "development"]
          },
          {
            "group": "Key Principles",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "Understanding Basic",
            "pages": ["essentials/settings", "essentials/navigation"],
            "expanded": true
          },
          {
            "group": "Advanced Topics",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "Windsurf Plugins",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      },
      {
        "tab": "Windsurf Reviews",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {}
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Feature Request",
        "href": "https://windsurf.canny.io/feature-requests/"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Sign In",
      "href": "https://windsurf.com/account/login"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "fonts": {
    "heading": {
      "family": "JetBrains Mono"
    },
    "body": {
      "family": "DM Sans"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
